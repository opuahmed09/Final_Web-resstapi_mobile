<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="facebook" parallel="false">


    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <!-- <parameter name="os_version" value="Sierra"/>-->
    <parameter name="platform" value="windows"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="74"/>
    <parameter name="url" value="https://www.facebook.com/"/>

    <test name="facebook">
        <parameter name="url" value ="https://www.facebook.com/"/>
        <classes>
            <class name="testfacebookhomepage.TestFacebookHomepage"/>
        </classes>
    </test>
</suite>